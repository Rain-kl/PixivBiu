# PixivBiu environment config example
# Priority: OS env > .env > config.yml > app/config/biu_default.yml
#
# Nested config key mapping rule:
#   biu.download.storageMode -> BIU_DOWNLOAD_STORAGEMODE
#   biu.download.immich.host -> BIU_DOWNLOAD_IMMICH_HOST

# System
# SYS_HOST=127.0.0.1:4001
# SYS_DEBUG=false
# SYS_AUTOOPEN=true

# Download storage mode: LOCAL | IMMICH
BIU_DOWNLOAD_STORAGEMODE=LOCAL

# Immich settings (required when storage mode is IMMICH)
BIU_DOWNLOAD_IMMICH_HOST=https://immich.example.com
BIU_DOWNLOAD_IMMICH_APIKEY=replace_with_api_key
BIU_DOWNLOAD_IMMICH_VERIFYTLS=true
BIU_DOWNLOAD_IMMICH_BASEPATH=/api
BIU_DOWNLOAD_IMMICH_TIMEOUTSEC=20
BIU_DOWNLOAD_IMMICH_RETRYMAX=3
